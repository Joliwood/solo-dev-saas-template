{
  "$schema": "http://json.schemastore.org/package",
  "name": "web",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "build:web": "NODE_ENV=production next build",
    "dev": "next dev --turbopack",
    "check": "pnpm typescript && pnpm eslint . && pnpm build:web",
