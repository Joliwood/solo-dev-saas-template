    "tailwindcss-animate": "1.0.7",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/client-preset": "^4.8.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@workspace/eslint_config": "workspace:^",
    "@workspace/typescript-config": "workspace:*",
    "autoprefixer": "^10.4.4",
    "eslint-config-next": "15.2.4",
    "postcss": "^8.4.12",
    "tailwindcss": "^4.1.4"
