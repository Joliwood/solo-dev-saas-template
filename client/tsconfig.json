{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "./.next/",
    "paths": {
      "#types": [
        "./types/"
      ],
      "#utils": [
        "./utils"
      ],
      "#components": [
        "./components"
      ],
      "#store": [
        "./store"
      ]
    }
  },
  "include": [
    "./pusrc/componentson",
    "./tailwind.config.cjs",
    ".eslintrc.js",
    "**/*.js",
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "__document.__tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "next",
    "node_modules"
  ]
}
