    "@tailwindcss/forms": "0.5.0",
    "@tailwindcss/typography": "0.5.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "eslint": "9.23.0",
    "graphql": "^16.10.0",
    "next": "15.2.4",
    "process": "0.11.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
